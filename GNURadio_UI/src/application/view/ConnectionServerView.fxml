<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1">
   <children>
      <Button fx:id="serverConnectButton" mnemonicParsing="false" prefHeight="66.0" prefWidth="52.0" text="Server" />
      <TextField fx:id="ipAddr" promptText="IP Address" />
      <TextField fx:id="port" promptText="Port" />
      <Button fx:id="connectButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Connect" />
   </children>
</AnchorPane>
